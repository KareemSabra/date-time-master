{
    "name": "time-master",
    "type": "module",
    "version": "0.0.0",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "description": "A powerful and flexible time manipulation and formatting library for JavaScript and TypeScript",
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "scripts": {
        "build": "bun run build.ts",
        "test": "vitest",
        "prepublishOnly": "bun run build",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "time",
        "date",
        "formatting",
        "manipulation",
        "typescript",
        "bun"
    ],
    "license": "MIT",
    "homepage": "https://github.com/kareemsabra/time-master#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kareemsabra/time-master.git"
    },
    "bugs": "https://github.com/kareemsabra/time-master/issues",
    "author": "Kareem Sabra",
    "devDependencies": {
        "bun-plugin-dts": "^0.3.0",
        "@types/bun": "^1.2.8",
        "vitest": "^1.3.1",
        "@vitest/coverage-v8": "^1.3.1",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "eslint-import-resolver-typescript": "^3.6.1"
    }
}
